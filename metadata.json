
{
  "name": "RangeMaster Pro",
  "description": "Profesjonalna aplikacja do obsługi zawodów strzeleckich z AI.",
  "version": "1.0.2",
  "requestFramePermissions": [
    "camera",
    "microphone"
  ],
  "expo": {
    "name": "RangeMaster Pro",
    "slug": "range-master-pro",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#030712"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Aplikacja potrzebuje dostępu do zdjęć, aby analizować tarcze strzeleckie."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#030712"
      }
    }
  }
}
